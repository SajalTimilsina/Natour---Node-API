{
  "version": 2,
  "builds": [
    {
      "src": "app.js",
      "use": "@vercel/node",
      "config": {
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.js"
    }
  ],
  "env": {
    "PORT": "3000",
    "NODE_ENV": "production",
    "DATABASE": "@database",
    "DATABASE_PASSWORD": "@database_password"
  }
}
